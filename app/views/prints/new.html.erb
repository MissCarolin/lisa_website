 <%= render 'shared/header' %>

<div class="new-container">
    <div class="container">
        <div class="column">
            <div class="row justify-content-center">
            <div class="col-6">
                <h1>New Print</h1>
                <%= simple_form_for(@print) do |f| %>
                <%= f.input :name %>
                <%= f.input :description %>
                <%= f.input :photo, as: :file, class: "btn btn-outline-secondary" %>
                <%= f.button :submit, class: "btn btn-secondary" %>
                <% end %>
            </div>
            </div>
        </div>
    </div>
</div>