<%= link_to picture_path(picture), class: "picture_link" do %>
<div class="cards">
    <%= cl_image_tag(picture.photo.key, crop: :fill, width: 400) %>
    <div class="card-category">
        <div class="content-text">
            <h2 class="card-headline"><%= picture.name %></h2>
            <p class ="description-card-text"><%= picture.description %></p>
        </div>
    </div>
</div>
<% end %>
